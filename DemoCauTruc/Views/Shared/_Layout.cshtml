<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Demo Cấu trúc thư mục</title>

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/dist/lib/bootstrap/dist/css/bootstrap.min.css" />

    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/dist/lib/fontawesome-free-6.4.0-web/css/all.min.css" />

    <!-- Bootstrap Datepicker CSS -->
    <link rel="stylesheet" href="~/dist/lib/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">

    <!-- Bootstrap Icons -->
    <link rel="stylesheet" href="~/dist/lib/bootstrap-icons/font/bootstrap-icons.css">

    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/dist/css/site.css" asp-append-version="true" />

    <!-- Thêm vào _Layout.cshtml hoặc trang view của bạn -->
    <link href="~/dist/lib/toastr/build/toastr.min.css" rel="stylesheet" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Demo cấu trúc</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="C0302GoiKham" asp-action="Index">Báo cáo thực hiện theo dõi gói khám bệnh</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div class="container" style="max-width:98%;margin:auto;">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @* <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2025 - Thai_Mau2 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer> *@
    <!-- jQuery -->
    <script src="~/dist/lib/jquery/dist/jquery.min.js"></script>

    <!-- Bootstrap Bundle (bao gồm Popper) -->
    <script src="~/dist/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    <!-- Bootstrap Datepicker JS -->
    <script src="~/dist/lib/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
    <script src="~/dist/lib/bootstrap-datepicker/dist/locales/bootstrap-datepicker.vi.min.js"></script>


    <!-- Inputmask -->
    <script src="~/dist/lib/inputmask/dist/jquery.inputmask.min.js"></script>

    <!-- Moment.js -->
    <script src="~/dist/lib/moment/min/moment.min.js"></script>

    <script src="~/dist/lib/toastr/build/toastr.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
