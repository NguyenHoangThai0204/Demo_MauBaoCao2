<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Demo Cấu trúc thư mục</title>

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/dist/css/bootstrap.min.css" />
	<link rel="stylesheet" href="~/dist/lib/litepicker/dist/css/litepicker.css" />
    <link rel="stylesheet" href="~/dist/lib/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Demo cấu trúc</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="C0302GoiKham" asp-action="Index">Báo cáo thực hiện theo dõi gói khám bệnh</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div class="container" style="max-width:98%;margin:auto;">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="/dist/js/BienChung.js"></script>
    <!-- jQuery -->
    <script src="@Url.Content("~/dist/lib/Jquery/jquery.min.js")"></script>
    <script src="@Url.Content("~/dist/lib/Jquery/jquery.inputmask.js")"></script>
    <!-- Bootstrap Datepicker JS -->
    <script src="~/dist/lib/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
    <script src="~/dist/lib/bootstrap-datepicker/dist/locales/bootstrap-datepicker.vi.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
